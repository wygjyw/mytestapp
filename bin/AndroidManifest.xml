        <activity
            android:name="com.example.mytestapp.Activity2"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.mytestapp.DlgActivity"
            android:label="@string/title_activity_dlg" >
        </activity>
    </application>

</manifest>
